{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "wphaven",
  "title": "Wp Haven",
  "description": "WPHaven Automations",
  "icon": "icon-rounded.png",
  "author": "itsjustxan",
  "owner": "embold",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "environments",
      "title": "Manage Environments",
      "description": "Automate common environments actions",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "title": "Personal Access Token",
      "name": "token",
      "description": "Generate in your WP Haven account",
      "type": "textfield",
      "required": true,
      "defaultValue": "zjCofa2eTYrFZXSaf35uycT6YirTrJnxM7Llfrb393405d4"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.56.0",
    "@raycast/utils": "^1.4.8",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/lodash": "^4.14.196",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "lodash": "^4.17.21",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
